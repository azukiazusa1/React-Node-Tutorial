 } items) \n        </h3>\n        :\n        $ {cartItems.reduce((a, c) => a + c.price * c.qty, 0)}\n        <button className=\"button primary\" disabled={cartItems.length === 0} onClick={checkoutHandler}>\n          Proceed to Checkout\n        </button>\n      </div>\n    </div>\n  )\n}\n\nexport default CartScreen","/Users/asaiippei/amazona/frontend/src/reducers/cardReducers.js",[],"/Users/asaiippei/amazona/frontend/src/constants/cartConstants.js",[],"/Users/asaiippei/amazona/frontend/src/actions/cartActions.js",[],"/Users/asaiippei/amazona/frontend/src/Screents/SigninScreen.js",["92"],"/Users/asaiippei/amazona/frontend/src/reducers/userReducers.js",[],"/Users/asaiippei/amazona/frontend/src/constants/userConstants.js",[],"/Users/asaiippei/amazona/frontend/src/actions/userAction.js",[],"/Users/asaiippei/amazona/frontend/src/Screents/RegisterScreen.js",["93","94"],{"ruleId":"95","replacedBy":"96"},{"ruleId":"97","replacedBy":"98"},{"ruleId":"99","severity":1,"message":"100","line":45,"column":15,"nodeType":"101","endLine":45,"endColumn":26},{"ruleId":"99","severity":1,"message":"100","line":48,"column":15,"nodeType":"101","endLine":48,"endColumn":26},{"ruleId":"95","replacedBy":"102"},{"ruleId":"97","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":17,"column":6,"nodeType":"106","endLine":17,"endColumn":8,"suggestions":"107"},{"ruleId":"104","severity":1,"message":"108","line":18,"column":6,"nodeType":"106","endLine":18,"endColumn":8,"suggestions":"109"},{"ruleId":"104","severity":1,"message":"110","line":21,"column":6,"nodeType":"106","endLine":21,"endColumn":8,"suggestions":"111"},{"ruleId":"104","severity":1,"message":"112","line":21,"column":6,"nodeType":"106","endLine":21,"endColumn":16,"suggestions":"113"},{"ruleId":"114","severity":1,"message":"115","line":11,"column":10,"nodeType":"116","messageId":"117","endLine":11,"endColumn":20},{"ruleId":"104","severity":1,"message":"112","line":23,"column":6,"nodeType":"106","endLine":23,"endCo